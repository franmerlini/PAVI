          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SueldosPorPerfilTableAdapter" GeneratorDataComponentClassName="SueldosPorPerfilTableAdapter" Name="SueldosPorPerfil" UserDataComponentName="SueldosPorPerfilTableAdapter">
            <MainSource>
